{
    "name": "magd-kudama/phatic-blog-extension",
    "description": "Blog generator for the Phatic static page generator",
    "keywords": ["PHP", "Static page generator", "Blogging"],
    "homepage": "http://www.magdkudama.com",
    "type": "project",
    "license": "MIT",

    "authors": [
        {
            "name": "Magd Kudama",
            "email": "magdkudama@gmail.com",
            "homepage": "http://www.magdkudama.com"
        }
    ],

    "require": {
        "php": ">=5.4.0",
        "magd-kudama/phatic": "1.0.*@dev"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.*@dev",
        "mockery/mockery": "0.8.0"
    },

    "minimum-stability": "dev",

    "autoload": {
        "psr-0": {
            "MagdKudama\\PhaticBlogExtension": "src/"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
